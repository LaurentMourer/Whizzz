<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.Historique" table="HISTORIQUE" schema="HR">
        <id name="id" column="ID"/>
        <property name="idrh" column="IDRH"/>
        <property name="action" column="ACTION"/>
        <property name="nom" column="NOM"/>
        <property name="type" column="TYPE"/>
        <property name="dateaction" column="DATEACTION"/>
        <property name="bonlivraison" column="BONLIVRAISON"/>
        <property name="actionmanuelle" column="ACTIONMANUELLE"/>
    </class>
</hibernate-mapping>