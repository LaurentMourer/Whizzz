<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.Utilisateur" table="UTILISATEUR" schema="HR">
        <id name="identifiant" column="IDENTIFIANT"/>
        <property name="motdepasse" column="MOTDEPASSE"/>
        <property name="nomuser" column="NOMUSER"/>
        <property name="prenomuser" column="PRENOMUSER"/>
        <property name="role" column="ROLE"/>
    </class>
</hibernate-mapping>